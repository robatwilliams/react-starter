{
  "name": "react-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preanalyze-bundle": "npm run build --silent -- --profile --json > bundle-stats.json",
    "analyze-bundle": "webpack-bundle-analyzer bundle-stats.json dist",
    "build": "webpack --config config/webpack.prod.multi.js",
    "serve": "http-server dist --gzip",
    "serveo": "npm run serve -- -o",
    "start": "webpack-dev-server --config config/webpack.dev.js",
    "starto": "npm run start -- --open"
  },
  "dependencies": {
    "@types/react": "^16.0.25",
    "@types/react-dom": "^16.0.3",
    "@types/react-router-dom": "^4.2.1",
    "awesome-typescript-loader": "^3.3.0",
    "clean-webpack-plugin": "^0.1.17",
    "compression-webpack-plugin": "^1.0.1",
    "core-js": "^2.5.1",
    "css-loader": "^0.28.7",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.10.0",
    "lodash": "^4.17.4",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-router-dom": "^4.2.2",
    "script-ext-html-webpack-plugin": "^1.8.8",
    "style-loader": "^0.19.0",
    "tslib": "^1.8.0",
    "typescript": "^2.6.1",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.9.4",
    "whatwg-fetch": "^2.0.3"
  },
  "engines": {
    "node": ">= 6",
    "npm": ">= 3"
  }
}
